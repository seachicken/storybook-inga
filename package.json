{
  "name": "storybook-inga",
  "version": "0.1.2",
  "description": "Storybook addon to visualize results analyzed with inga",
  "main": "dist/register.js",
  "scripts": {
    "storybook": "npm run build && start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seachicken/storybook-inga.git"
  },
  "author": "Seito Tanaka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seachicken/storybook-inga/issues"
  },
  "homepage": "https://github.com/seachicken/storybook-inga#readme",
  "keywords": [
    "inga",
    "storybook",
    "addon"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
